{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",
    "_version": "16.1.0",
  
    "compilerOptions": {
      "lib": ["ESNext", "DOM"],
      "module": "Node16",
      "target": "ESNext",
  
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node16"
    }
}